//@execute =0
class A(x: int) { }

class B extends A {
	def y: int;

	new(x: int, y) super(x) { }
}

type C #unboxed {
	case X(a: A);
	case Y(b: B);
}

def main() -> int {
	var x = C.X(A.new(12));
	var y = C.Y(B.new(34, 56));

	if (x.a.x != 12) return -1;
	if (y.b.x != 34) return -2;
	if (y.b.y != 56) return -3;
	return 0;
}