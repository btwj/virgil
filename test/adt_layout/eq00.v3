//@execute =0
type A00 #unboxed {
	case X;
	case Y(x: int);
}

def eq(a: A00, b: A00) -> bool {
	return a == b;
}

def main() -> int {
	var a00_x: A00 = A00.X;
	var a00_x2: A00 = A00.X;
	var a00_y: A00 = A00.Y(12);
	var a00_y2: A00 = A00.Y(12);
	var a00_y3: A00 = A00.Y(13);

	if (!eq(a00_x, a00_x2)) return -1;
	if (eq(a00_x, a00_y)) return -2;
	if (!eq(a00_y, a00_y2)) return -3;
	if (eq(a00_y, a00_y3)) return -4;
	return 0;
}
