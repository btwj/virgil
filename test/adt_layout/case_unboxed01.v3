//@execute =0
type A00 {
	case X(x: u32) #unboxed;
	case Y(x: u32, y: u32);
}

type A01 {
	case X(x: u32);
	case Y(x: u32, y: u32) #unboxed;
}

type A02 {
	case X(x: u32) #unboxed;
	case Y(x: u32, y: u32) #unboxed;
}

def main() -> u32 {
	var a00_x = A00.X(99);
	var a00_y = A00.Y(11, 9);

	var a01_x = A01.X(88);
	var a01_y = A01.Y(22, 19);

	var a02_x = A02.X(77);
	var a02_y = A02.Y(33, 29);

	// Prevent fields from getting eliminated
	var sum: u32 = 0;
        match (a00_x) { X(x) => sum += x; Y(x, y) => sum += x + y; }
        match (a01_x) { X(x) => sum += x; Y(x, y) => sum += x + y; }
        match (a02_x) { X(x) => sum += x; Y(x, y) => sum += x + y; }

	return sum;
}